{
  "name": "nakedobjects.spa",
  "version": "13.0.0-beta01",
  "description": "Single Page Application client for a Naked Objects application.",
  "license": "Apache-2.0",
  "author": "Richard Pawson <rpawson@nakedobjects.org>",
  "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
  "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
  "repository": {
    "type": "git",
    "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
  },
  "keywords": [
    "rest",
    "restful",
    "objects",
    "naked",
    "nakedobjects",
    "ddd",
    "domain",
    "driven",
    "design",
    "api"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch false --progress false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepackage": "node prepackage.js",
    "compile:restful-objects": "yarn && rimraf dist/nakedobjects-restful-objects && ng-packagr -p restful-objects/package.json",
    "pack:restful-objects": "cd dist/nakedobjects-restful-objects && yarn pack && cd ../..",
    "install:restful-objects": "yarn add file:./dist/nakedobjects-restful-objects/nakedobjects-restful-objects-v13.0.0-beta01.tgz",
    "build:restful-objects": "yarn compile:restful-objects && yarn pack:restful-objects && yarn install:restful-objects",
    "compile:services": "yarn && rimraf dist/nakedobjects-services && ng-packagr -p services/package.json",
    "pack:services": "cd dist/nakedobjects-services && yarn pack && cd ../..",
    "install:services": "yarn add file:./dist/nakedobjects-services/nakedobjects-services-v13.0.0-beta01.tgz",
    "build:services": "yarn compile:services && yarn pack:services && yarn install:services",
    "compile:view-models": "yarn && rimraf dist/nakedobjects-view-models && ng-packagr -p view-models/package.json",
    "pack:view-models": "cd dist/nakedobjects-view-models && yarn pack && cd ../..",
    "install:view-models": "yarn add file:./dist/nakedobjects-view-models/nakedobjects-view-models-v13.0.0-beta01.tgz",
    "build:view-models": "yarn compile:view-models && yarn pack:view-models && yarn install:view-models",
    "compile:gemini": "yarn && rimraf dist/nakedobjects-gemini && ng-packagr -p gemini/package.json",
    "pack:gemini": "cd dist/nakedobjects-gemini && yarn pack && cd ../..",
    "install:gemini": "yarn add file:./dist/nakedobjects-gemini/nakedobjects-gemini-v13.0.0-beta01.tgz",
    "build:gemini": "yarn compile:gemini && yarn pack:gemini && yarn install:gemini",
    "compile:cicero": "yarn && rimraf dist/nakedobjects-cicero && ng-packagr -p cicero/package.json",
    "pack:cicero": "cd dist/nakedobjects-cicero && yarn pack && cd ../..",
    "install:cicero": "yarn add file:./dist/nakedobjects-cicero/nakedobjects-cicero-v13.0.0-beta01.tgz",
    "build:cicero": "yarn && yarn compile:cicero && yarn pack:cicero && yarn install:cicero",
    "build:clean": "rimraf dist && rimraf node_modules/@nakedobjects && yarn cache clean",
    "build:libs": "yarn lint && yarn build:clean && yarn build:restful-objects && yarn build:services && yarn build:view-models && yarn build:gemini && yarn build:cicero",
    "build:app": "ng build packages --no-progress",
    "build:app:prod": "ng build packages --no-progress --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.3",
    "@angular/cdk": "~13.2.3",
    "@angular/common": "~13.2.3",
    "@angular/compiler": "~13.2.3",
    "@angular/core": "~13.2.3",
    "@angular/forms": "~13.2.3",
    "@angular/platform-browser": "~13.2.3",
    "@angular/platform-browser-dynamic": "~13.2.3",
    "@angular/router": "~13.2.3",
    "auth0-js": "^9.19.0",
    "lodash": "~4.17.21",
    "lodash-es": "~4.17.21",
    "moment": "~2.29.1",
    "rxjs": "~7.5.0",
    "tslib": "~2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.4",
    "@angular/compiler-cli": "~13.2.3",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "~4.14.157",
    "@types/lodash-es": "~4.17.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "cpr": "^3.0.1",
    "eslint": "^8.2.0",
    "find-in-files": "^0.5.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "~13.2.1",
    "replace-in-file": "~6.1.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.5.5"
  }
}
