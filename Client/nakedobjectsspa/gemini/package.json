{
    "ngPackage": {
        "$schema": "./node_modules/ng-packagr/package.schema.json",
        "lib": {
            "entryFile": "src/public_api.ts",
            "umdModuleIds": {
                "lodash": "_",
                "moment": "moment",
                "moment-timezone": "moment-timezone",
                "lodash-es/difference": "difference",
                "lodash-es/findIndex": "findIndex",
                "lodash-es/first": "first",
                "lodash-es/every": "every",
                "lodash-es/keys": "keys",
                "lodash-es/omit": "omit",
                "lodash-es/filter": "filter",
                "lodash-es/flatten": "flatten",
                "lodash-es/mapValues": "mapValues",
                "lodash-es/zipObject": "zipObject",
                "lodash-es/each": "each",
                "lodash-es/find": "find",
                "lodash-es/forEach": "forEach",
                "lodash-es/map": "map",
                "lodash-es/some": "some",
                "lodash-es/concat": "concat",
                "@nakedobjects/view-models": "@nakedobjects/view-models",
                "@nakedobjects/services": "@nakedobjects/services",
                "@nakedobjects/restful-objects": "@nakedobjects/restful-objects"
            }
        },
        "allowedNonPeerDependencies": [
            "auth0-js",
            "lodash",
            "lodash-es",
            "moment",
            "tslib"
        ],
        "dest": "../dist/nakedobjects-gemini"
    },
    "name": "@nakedobjects/gemini",
    "version": "12.0.0-rc.1",
    "description": "Single Page Application client for a Naked Objects application.",
    "license": "Apache-2.0",
    "author": "Richard Pawson <rpawson@nakedobjects.org>",
    "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
    "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
    "repository": {
        "type": "git",
        "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
    },
    "angular-cli": {},
    "keywords": [
        "rest",
        "restful",
        "objects",
        "naked",
        "nakedobjects",
        "ddd",
        "domain",
        "driven",
        "design",
        "api"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "auth0-js": "^9.16.1",
        "lodash": "~4.17.21",
        "lodash-es": "~4.17.21",
        "moment": "~2.29.1",
        "tslib": "~2.3.0"
    },
    "peerDependencies": {
        "@angular/animations": "~12.1.1",
        "@angular/cdk": "~12.1.1",
        "@angular/common": "~12.1.1",
        "@angular/compiler": "~12.1.1",
        "@angular/core": "~12.1.1",
        "@angular/forms": "~12.1.1",
        "@angular/platform-browser": "~12.1.1",
        "@angular/platform-browser-dynamic": "~12.1.1",
        "@angular/router": "~12.1.1",
        "@nakedobjects/restful-objects": "12.0.0-rc.1",
        "@nakedobjects/services": "12.0.0-rc.1",
        "@nakedobjects/view-models": "12.0.0-rc.1",
        "rxjs": "~6.6.7",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.1.1",
        "@angular/cli": "~12.1.1",
        "@angular/compiler-cli": "~12.1.1",
        "@angular/language-service": "~12.1.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash": "~4.14.157",
        "@types/lodash-es": "~4.17.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.2",
        "jasmine-core": "~3.7.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.6.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.3",
        "typescript": "~4.3.4"
    }
}