<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Property - Virtual</Title>
      <Author>Naked Objects Group</Author>
      <Description>Adds an auto-property marked as virtual.</Description>
      <HelpUrl>www.nakedobjects.net</HelpUrl>
      <SnippetTypes />
      <Keywords />
      <Shortcut>propv</Shortcut>
    </Header>
    <Snippet>
      <References />
      <Imports />
      <Declarations>
        <Literal Editable="true" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
          <ID>Name</ID>
          <Type />
          <ToolTip>Enter the name for the property</ToolTip>
          <Default>Name</Default>
          <Function />
        </Literal>
        <Literal Editable="true" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
          <ID>Type</ID>
          <Type />
          <ToolTip>Enter the Type of the Property</ToolTip>
          <Default>string</Default>
          <Function />
        </Literal>
      </Declarations>
      <Code Language="CSharp" Kind="method decl" Delimiter="$"><![CDATA[
      public virtual $Type$ $Name$ { get; set;}
      ]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>