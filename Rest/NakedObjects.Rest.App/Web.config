  <connectionStrings>
    <add name="RestTest" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=RestTest;Integrated Security=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings />
