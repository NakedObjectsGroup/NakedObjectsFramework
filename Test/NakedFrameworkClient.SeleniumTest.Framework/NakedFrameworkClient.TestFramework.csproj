<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>Internal Selenium Helpers</Title>
		<Authors>Naked Objects Group</Authors>
		<Company>Naked Objects Group</Company>
		<Product>Naked Objects</Product>
		<Description>Internal Selenium Helpers</Description>
		<OutputType>Library</OutputType>
		<IsPublishable>True</IsPublishable>
		<Copyright>Naked Objects Group</Copyright>
		<PackageProjectUrl>https://github.com/NakedObjectsGroup</PackageProjectUrl>
		<RepositoryUrl>https://github.com/NakedObjectsGroup</RepositoryUrl>
		<Version>112.2.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="drivers\chromedriver.exe" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="drivers\chromedriver.exe" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MSTest.TestFramework" Version="3.0.2" />
		<PackageReference Include="Selenium.Support" Version="4.8.2" />
		<PackageReference Include="Selenium.WebDriver" Version="4.8.2" />
	</ItemGroup>

</Project>